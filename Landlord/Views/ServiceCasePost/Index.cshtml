@model IEnumerable<Landlord.Models.ServiceCasePostViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CaseNr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CaseMessage)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CaseNr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CaseMessage)
        </td>
    </tr>
}
</table>
